mixin alert-success(time)
    - if (messages.Success)
            .message.info
                div(class="alert alert-success" show-alert  data-time=time)
                    != messages.Success 
                    <span close-alert style="margin-left:25px ;cursor: pointer;" >X</span>



mixin alert-error(time)
    - if (messages.error)
            .message.info
                div(class="alert alert-danger" show-alert  data-time=time)
                    != messages.error 
                    <span close-alert style="margin-left:25px ;cursor: pointer;" >X</span>